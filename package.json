    "axios": "^1.5.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.8.3",
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4",
    "element-ui": "^2.15.14",
    "fs": "^0.0.1-security",
    "jsencrypt": "^3.3.2",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
    "vue": "^2.7.14",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2",
    "vxe-table": "3.6.14-beta.3",
